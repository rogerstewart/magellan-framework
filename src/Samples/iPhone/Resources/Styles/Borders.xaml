<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Style x:Key="Border.iPhoneOuterBorderStyle" TargetType="{x:Type Border}">
		<Setter Property="Background" Value="Black" />
		<Setter Property="BorderThickness" Value="3" />
		<Setter Property="CornerRadius" Value="20" />
		<Setter Property="BorderBrush">
			<Setter.Value>
				<RadialGradientBrush GradientOrigin="-0.055,0.5" RadiusX="0.697" RadiusY="0.697">
	    			<GradientStop Color="#FF454545" Offset="0"/>
	    			<GradientStop Color="#FF323232" Offset="1"/>
	    			<GradientStop Color="#FFA7A7A3" Offset="0.617"/>
	    			<GradientStop Color="#FF686868" Offset="0.295"/>
	    		</RadialGradientBrush>
			</Setter.Value>
		</Setter>
	</Style>
	<!-- Resource dictionary entries should be defined here. -->
</ResourceDictionary>