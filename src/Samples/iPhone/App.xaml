    			<ResourceDictionary Source="Resources/Styles/Borders.xaml"/>
    			<ResourceDictionary Source="Resources/Styles/Frames.xaml"/>
    			<ResourceDictionary Source="Resources/Styles/Pages.xaml"/>
    			<ResourceDictionary Source="Resources/Styles/Tabs.xaml"/>
    			<ResourceDictionary Source="Resources/Styles/PageTitles.xaml"/>
    		</ResourceDictionary.MergedDictionaries>
    	</ResourceDictionary>
         
    </Application.Resources>
</Application>
